{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "a6digital/laravel-default-profile-image": "^2.0",
        "anhskohbo/no-captcha": "^3.5",
        "astrotomic/laravel-translatable": "^11.12",
        "convertapi/convertapi-php": "^3.0",
        "doctrine/dbal": "^3.6",
        "fruitcake/laravel-cors": "^2.0",
        "geoip2/geoip2": "3.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.5",
        "laravel/framework": "^8.75",
        "laravel/passport": "^10.4",
        "laravel/sanctum": "^2.15",
        "laravel/tinker": "^2.5",
        "mcamara/laravel-localization": "^1.8",
        "php-flasher/flasher-toastr-laravel": "^1.15",
        "realrashid/sweet-alert": "^7.2",
        "santigarcor/laratrust": "^7.1",
        "spatie/laravel-permission": "^6.9",
        "spatie/laravel-translatable": "^5.2",
        "taqnyat/php": "1.0",
        "torann/geoip": "*",
        "tymon/jwt-auth": "^1.0@dev",
        "yajra/laravel-datatables-oracle": "~9.0",
        "yoeunes/toastr": "^2.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }, "files":[
            "app/Helper/Helper.php",
            "app/Helper/GoogleMaps.php",
            "app/Helper/MainHelper.php"

        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
